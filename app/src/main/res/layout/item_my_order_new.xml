<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="4dp"

    android:background="@drawable/for_white"
    tools:visibility="visible">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout32"
        android:layout_width="0dp"
        android:layout_height="wrap_content"

        android:layout_marginStart="5dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="5dp"
        android:background="@drawable/bg_card_round_green"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <Button
            android:id="@+id/textView282"
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="8dp"
            android:backgroundTint="@color/colorPrimaryNew"
            android:padding="2dp"
            android:text="Reorder"
            android:textSize="12dp"
            android:visibility="gone"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/button24"
            app:layout_constraintTop_toBottomOf="@+id/textView330" />

        <TextView
            android:id="@+id/textView323"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"

            android:textColor="@color/black"
            android:textSize="14dp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView327"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="8dp"

            android:textColor="@color/colorPrimaryNew"
            android:textSize="@dimen/_10sdp"
            app:layout_constraintBottom_toBottomOf="@+id/textView323"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/textView323" />

        <TextView
            android:id="@+id/textView329"
            android:layout_width="0dp"
            android:layout_height="21dp"
            android:layout_marginTop="8dp"

            android:textColor="@color/black"
            android:textSize="11dp"
            app:layout_constraintStart_toStartOf="@+id/textView323"
            app:layout_constraintTop_toBottomOf="@+id/textView323" />

        <TextView
            android:id="@+id/textView330"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="9dp"
            app:layout_constraintBottom_toBottomOf="@+id/textView329"
            app:layout_constraintEnd_toEndOf="@+id/textView327"
            app:layout_constraintTop_toBottomOf="@+id/textView323" />

        <TextView
            android:id="@+id/textView336"
            android:layout_width="0dp"
            android:layout_height="13dp"
            android:layout_marginTop="2dp"
            android:textAllCaps="true"
            android:textColor="@color/credit_green"
            android:textSize="12dp"
            app:layout_constraintStart_toStartOf="@+id/textView329"
            app:layout_constraintTop_toBottomOf="@+id/textView329" />

        <TextView
            android:id="@+id/textView351"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="8dp"
            android:textColor="@color/colorPrimaryNew"
            android:textSize="@dimen/_10sdp"
            android:visibility="invisible"
            app:layout_constraintBottom_toBottomOf="@+id/textView323"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/textView323" />

        <TextView
            android:id="@+id/textView392"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:textColor="@color/debit_red"

            android:textSize="12dp"
            app:layout_constraintStart_toEndOf="@+id/textView336"
            app:layout_constraintTop_toTopOf="@+id/textView336" />

        <Button
            android:id="@+id/button24"
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:backgroundTint="@color/debit_red"
            android:padding="2dp"
            android:text="Return"
            android:textSize="12dp"
            android:visibility="gone"
            app:layout_constraintBottom_toBottomOf="@+id/textView282"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/textView282" />

        <TextView
            android:id="@+id/returnstatus"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:layout_marginBottom="1dp"
            android:textColor="@color/debit_red"

            android:textSize="12dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="@+id/textView336"
            app:layout_constraintTop_toBottomOf="@+id/textView336" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cons_new"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/quantum_white_100"
        android:visibility="gone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout32">

        <TextView
            android:id="@+id/textView371"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:gravity="center"
            android:paddingStart="4dp"
            android:paddingEnd="8dp"
            android:text="Item"
            android:textColor="#BE000000"
            android:textStyle="bold"
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/textView375"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <TextView
            android:id="@+id/textView375"
            android:layout_width="60dp"
            android:layout_height="wrap_content"
            android:gravity="start"
            android:paddingEnd="@dimen/_8sdp"
            android:text="Unit"
            android:textColor="#BE000000"
            android:textStyle="bold"
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/textView377"
            app:layout_constraintStart_toEndOf="@+id/textView371"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView377"
            android:layout_width="40dp"
            android:layout_height="wrap_content"
            android:gravity="center"

            android:text="Price"
            android:textColor="#BE000000"
            android:textStyle="bold"
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/textView383"
            app:layout_constraintStart_toEndOf="@+id/textView375"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView383"
            android:layout_width="40dp"
            android:layout_height="wrap_content"
            android:gravity="end"
            android:paddingEnd="@dimen/_2sdp"
            android:text="Qty"
            android:textColor="#BE000000"
            android:textSize="14sp"
            android:textStyle="bold"
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textView377"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView396"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="24dp"
            android:gravity="center_horizontal"

            android:text="Total"
            android:textColor="#BE000000"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textView383"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recy_two"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/bg_card_round"
        android:padding="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cons_new"
        tools:listitem="@layout/item_myitem_two" />

    <View
        android:id="@+id/view17"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:background="@color/iron"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/recy_two" />


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/view28"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:background="@color/quantum_white_100"
        android:padding="8dp"
android:visibility="gone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view17">

        <TextView
            android:id="@+id/textView340"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:gravity="end"
            android:text="Delivery Charge"
            android:textColor="@color/black"
            android:visibility="visible"
            app:layout_constraintEnd_toEndOf="@+id/textView346"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView341"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="24dp"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="@+id/textView340"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/textView340" />

        <TextView
            android:id="@+id/textView342"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:gravity="end"
            android:text="Packing Charge"
            android:textColor="@color/black"
            android:visibility="visible"
            app:layout_constraintEnd_toEndOf="@+id/textView346"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView340" />


        <TextView
            android:id="@+id/textView343"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="24dp"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="@+id/textView346"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/textView346" />

        <TextView
            android:id="@+id/textView345"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="24dp"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="@+id/textView342"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/textView342" />

        <TextView
            android:id="@+id/textView346"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="32dp"
            android:layout_marginBottom="8dp"
            android:gravity="end"
            android:text="Total"
            android:textColor="@color/black"
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/textView343"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView401" />

        <TextView
            android:id="@+id/textView401"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:gravity="end"
            android:text="Discount"
            android:textColor="@color/black"
            app:layout_constraintEnd_toEndOf="@+id/textView346"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView342" />

        <TextView
            android:id="@+id/textView402"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="24dp"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="@+id/textView401"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/textView401" />

    </androidx.constraintlayout.widget.ConstraintLayout>




    <TextView
        android:id="@+id/textView389"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Requested Order Details"
        android:textColor="@color/green_drawer"
        android:textSize="14dp"
        android:visibility="gone"
        app:layout_constraintEnd_toEndOf="@+id/recy_two"
        app:layout_constraintTop_toBottomOf="@+id/view28" />

    <Button
        android:id="@+id/confirm"
        android:layout_width="160dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:text="Confirm"

        android:visibility="gone"
        app:layout_constraintEnd_toStartOf="@+id/cancel_after"
        app:layout_constraintTop_toTopOf="@+id/cancel_after" />

    <Button
        android:id="@+id/cancel_after"
        android:layout_width="160dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:backgroundTint="@color/debit_red"
        android:text="Cancel"

        android:visibility="gone"
        app:layout_constraintEnd_toEndOf="@+id/textView389"
        app:layout_constraintTop_toBottomOf="@+id/textView389" />

    <Button
        android:id="@+id/cancel_edit"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:layout_marginEnd="50dp"
        android:backgroundTint="@color/debit_red"
        android:text="Cancel"
        android:visibility="gone"
        app:layout_constraintEnd_toEndOf="@+id/constraintLayout32"
        app:layout_constraintTop_toBottomOf="@+id/view28" />

    <Button
        android:id="@+id/cancellall"
        android:layout_width="150dp"
        android:layout_height="40dp"
        android:layout_marginEnd="50dp"
        android:backgroundTint="@color/debit_red"
        android:text="Cancel all"

       android:visibility="gone"
        app:layout_constraintEnd_toEndOf="@+id/constraintLayout32"
        app:layout_constraintTop_toBottomOf="@+id/view28" />

    <Button
        android:id="@+id/button31"
        android:layout_width="wrap_content"
        android:layout_height="35dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:backgroundTint="@color/colorPrimaryNew"
android:visibility="gone"
        android:padding="2dp"
        android:text="Submit"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view28" />

    <Button
        android:id="@+id/button34"
        android:layout_width="wrap_content"
        android:layout_height="35dp"
        android:layout_marginStart="8dp"
        android:backgroundTint="@color/debit_red"
android:visibility="gone"
        android:padding="2dp"
        android:text="Cancel"
        app:layout_constraintBottom_toBottomOf="@+id/button31"
        app:layout_constraintStart_toEndOf="@+id/button31"
        app:layout_constraintTop_toTopOf="@+id/button31" />

    <View
        android:id="@+id/view30"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:layout_marginBottom="8dp"
        android:background="@color/iron"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>